<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.user.rmate-server</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/rmate_server</string>
    </array>
    
    <key>EnvironmentVariables</key>
    <dict>
        <key>RMATE_EDITOR</key>
        <string>code --wait</string>
        <key>RMATE_PORT</key>
        <string>52698</string>
        <key>RMATE_IP</key>
        <string>127.0.0.1</string>
    </dict>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>KeepAlive</key>
    <true/>
    
    <key>StandardOutPath</key>
    <string>/usr/local/var/log/rmate-server.log</string>
    
    <key>StandardErrorPath</key>
    <string>/usr/local/var/log/rmate-server-error.log</string>
    
    <key>WorkingDirectory</key>
    <string>/tmp</string>
</dict>
</plist>